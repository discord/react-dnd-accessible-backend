{
  "name": "react-dnd-accessible-backend",
  "version": "1.0.1",
  "author": "Jon Egeland <jon.egeland@discordapp.com>",
  "description": "An add-on backend for react-dnd that provides support for keyboards and screenreaders by default.",
  "license": "MIT",
  "repository": "discord/react-dnd-accessible-backend",
  "homepage": "https://github.com/discord/react-dnd-accessible-backend#readme",
  "keywords": [
    "a11y",
    "keyboard",
    "react",
    "react-dnd",
    "dnd",
    "sortable",
    "drag-and-drop",
    "drag-n-drop",
    "drag-drop",
    "drag",
    "drop",
    "typescript"
  ],
  "files": [
    "src",
    "dist"
  ],
  "source": "index.tsx",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [],
  "exports": {
    ".": {
      "import": "./index.tsx",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js"
    }
  },
  "scripts": {
    "format": "prettier --write '**'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dnd": ">=14.0.0"
  },
  "devDependencies": {
    "@types/react": "17.0.26",
    "@types/react-dom": "17.0.9",
    "dnd-core": "16.0.1",
    "prettier": "^2.0.4",
    "react": "^18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dnd-multi-backend": "8.0.0",
    "react-dom": "^18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.13.0"
  },
  "bugs": {
    "url": "https://github.com/discord/react-dnd-accessible-backend/issues"
  },
  "dependencies": {
    "@react-aria/live-announcer": "^3.3.0"
  }
}
